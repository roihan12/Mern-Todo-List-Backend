{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
   // Add extra exclusions for the @typescript-eslint
  ],
  "rules": {
    "no-console": ["warn", { "allow": ["info"] }]
    // "prettier/prettier": "error"
  }
}
